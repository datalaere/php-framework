{
    "name": "datalaere/php-skeleton",
    "type": "library",
    "description": "PHP skeleton.",
    "keywords": [
        "skeleton",
        "library"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.1",
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/src/"
        },
        "classmap": [
            "app/src/"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "app/tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
}
