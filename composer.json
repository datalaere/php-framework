{
    "name": "datalaere/php-project",
    "type": "library",
    "description": "PHP default project.",
    "keywords": [
        "skeleton",
        "library"
    ],
    "license": "MIT",
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/datalaere/php-mvc"
        },
        {
            "type": "vcs",
            "url": "https://github.com/datalaere/php-di"
        },
        {
            "type": "vcs",
            "url": "https://github.com/datalaere/php-crud"
        },
        {
            "type": "vcs",
            "url": "https://github.com/datalaere/php-http"
        }
    ],
    "require-dev": {
        "phpunit/phpunit": "^8.0"
    },
    "require": {
        "php": ">=5.5.0",
        "datalaere/php-mvc": "dev-master",
        "datalaere/php-di": "dev-master",
        "datalaere/php-http": "dev-master",
        "datalaere/php-crud": "dev-master"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "app/tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
